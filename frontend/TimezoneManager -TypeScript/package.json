{
  "name": "TimezoneManager",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest __tests__",
    "lint": "eslint ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix --fix-type problem",
      "git add"
    ]
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.7.1",
    "@types/lodash": "^4.14.155",
    "@types/lodash.memoize": "^4.1.6",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/redux-persist-transform-encrypt": "^2.0.1",
    "axios": "^0.19.1",
    "date-fns": "^2.11.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.memoize": "^4.1.2",
    "prop-types": "^15.7.2",
    "react": "16.11.0",
    "react-dom": "^16.12.0",
    "react-native": "0.62.1",
    "react-native-clock-analog": "0.0.1",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-reanimated": "^1.4.0",
    "react-native-safe-area-context": "^0.6.4",
    "react-native-safe-area-view": "^1.0.0",
    "react-native-screens": "^1.0.0-alpha.23",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^4.0.10",
    "react-navigation-drawer": "^2.4.6",
    "react-navigation-stack": "^1.10.3",
    "react-redux": "^7.1.3",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-encrypt": "^2.0.1",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/runtime": "7.9.2",
    "@react-native-community/eslint-config": "0.0.5",
    "@react-native-community/eslint-plugin": "^1.0.0",
    "@types/jest": "^25.2.3",
    "@types/react": "^16.9.35",
    "@types/react-native": "^0.62.11",
    "@types/react-redux": "^7.1.9",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "24.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "detox": "^16.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "eslint": "6.8.0",
    "eslint-config-recommended": "^4.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.3.0",
    "husky": "^4.2.3",
    "jest": "24.9.0",
    "lint-staged": "^8.2.1",
    "metro-react-native-babel-preset": "0.56.4",
    "prettier": "2.0.5",
    "react-native-dotenv": "^0.2.0",
    "react-test-renderer": "16.9.0",
    "redux-mock-store": "^1.5.4",
    "rerender-tracker": "^1.0.12",
    "typescript": "^3.9.3"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/setup/enzymeSetup.js",
      "<rootDir>/__tests__/setup/logSetup.js",
      "<rootDir>/__tests__/setup/storeSetup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/__mocks__/",
      "<rootDir>/__tests__/setup/",
      "<rootDir>/node_modules/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation|@react-navigation/native/)"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "html"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/services/navigation",
      "<rootDir>/src/services/api",
      "<rootDir>/src/store/middlewares",
      "<rootDir>/src/store/sagas/rootSaga",
      "<rootDir>/src/store/reducers/rootReducer"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/TimezoneManager.app",
        "build": "xcodebuild -workspace ios/TimezoneManager.xcworkspace -scheme TimezoneManager -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "device": {
          "type": "iPhone X"
        }
      }
    },
    "test-runner": "jest"
  }
}
